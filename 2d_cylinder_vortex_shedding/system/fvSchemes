FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "$FOAM_CASE/include/initialConditions"

ddtSchemes
{
    #if $transient
        default         CrankNicolson 0.9;
    #else
        default         steadyState;
    #endif
}

d2dt2Schemes
{
    #if $transient
        default         Euler;
    #else
        default         steadyState;
    #endif
}

gradSchemes
{
    default         Gauss linear;

    grad(U)         cellLimited Gauss linear 1;
}

divSchemes
{
    default         none;

    #if $transient
        strategy            Gauss limitedLinear 1.0;

        div(phi,U)          Gauss limitedLinearV 1.0;
    #else
        div(phi,U)      bounded Gauss limitedLinear 1.0;
    #endif
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

// ************************************************************************* //
