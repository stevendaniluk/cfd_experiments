     \\/     M anipulation  |
\*---------------------------------------------------------------------------*/
FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

#include        "../include/initialConditions"

internalField   uniform ($U_inf 0 0);

boundaryField
{
    // Inlet
    X_min
    {
        type            fixedValue;
        value           $internalField;
    }

    // Outlet
    X_max
    {
        type            zeroGradient;
    }

    // Walls
    "(Y_min|Y_max)"
    {
        type            symmetryPlane;
    }
    "(Z_min|Z_max)"
    {
        type            empty;
    }

    body
    {
        type            noSlip;
    }

}

// ************************************************************************* //
